#!/bin/bash
#SBATCH --job-name=knn_slak_7x7
#SBATCH -p gpu
#SBATCH -N 1
#SBATCH --ntasks-per-node=1
#SBATCH --gpus-per-node=1
#SBATCH --gpus=1
#SBATCH -t 0-0:59:59
#SBATCH --cpus-per-task=18
#SBATCH -o knn_slak_7x7.out

source /home/sliu/miniconda3/etc/profile.d/conda.sh
source activate LoRA

python -m torch.distributed.launch --nproc_per_node=1 eval_knn.py --data_path /projects/2/managed_datasets/imagenet/ \
--arch SLaK_tiny --kernel_size 7 7 7 7 5 --LoRA True \
--pretrained_weights /projects/0/prjste21060/projects/dino//projects/0/prjste21060/projects/dino/dino_slak_7/checkpoint.pth



source deactivate