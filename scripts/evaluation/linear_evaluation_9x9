#!/bin/bash
#SBATCH --job-name=linear_slak_9x9_300epochs
#SBATCH -p gpu
#SBATCH -N 1
#SBATCH --ntasks-per-node=1
#SBATCH --gpus-per-node=1
#SBATCH --gpus=1
#SBATCH -t 2-23:59:59
#SBATCH --cpus-per-task=18
#SBATCH -o linear_slak_9x9_300epochs.out

source /home/sliu/miniconda3/etc/profile.d/conda.sh
source activate slak


python -m torch.distributed.launch --nproc_per_node=1 --master_port=56484  eval_linear.py --data_path /projects/2/managed_datasets/imagenet/ \
--arch SLaK_tiny --kernel_size 9 9 9 9 100 --LoRA False --bn True \
--pretrained_weights /projects/0/prjste21060/projects/dino/dino_slak_9_bn_300/checkpoint.pth \
--output_dir /projects/0/prjste21060/projects/dino/dino_slak_9_bn/linear_300/ --checkpoint_key teacher

source deactivate