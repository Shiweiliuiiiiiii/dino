#!/bin/bash
#SBATCH --job-name=eval_knn_dino_vit_small_64
#SBATCH -p gpu
#SBATCH -N 1
#SBATCH --ntasks-per-node=1
#SBATCH --gpus-per-node=1
#SBATCH --gpus=1
#SBATCH -t 0-0:59:59
#SBATCH --cpus-per-task=18
#SBATCH -o eval_knn_dino_vit_small_64.out

source /home/sliu/miniconda3/etc/profile.d/conda.sh
source activate LoRA

python -m torch.distributed.launch --nproc_per_node=1 eval_knn.py --data_path /projects/2/managed_datasets/imagenet/ \
--arch vit_small  \
--pretrained_weights /projects/0/prjste21060/projects/dino/dino_vit_small_64/checkpoint.pth


source deactivate